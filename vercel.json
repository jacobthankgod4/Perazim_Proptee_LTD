{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "public": false,
  "builds": [
    {
      "src": "public_html/**/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "public_html/assets/**",
      "use": "@vercel/static"
    },
    {
      "src": "public_html/JavaScript/**",
      "use": "@vercel/static"
    },
    {
      "src": "public_html/i/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/public_html/assets/$1"
    },
    {
      "src": "/JavaScript/(.*)",
      "dest": "/public_html/JavaScript/$1"
    },
    {
      "src": "/i/(.*)",
      "dest": "/public_html/i/$1"
    },
    {
      "src": "/(.*\\.php)",
      "dest": "/public_html/$1"
    },
    {
      "src": "/((?!api|_next|_static|favicon\\.ico).*)",
      "dest": "/public_html/index.php"
    }
  ],
  "env": {}
}
